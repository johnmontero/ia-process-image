@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> 
    <link href="~/Content/styles.css?=1233232" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div class="modal"></div>
    <h3>
        Demo Inteligencia Artificial
    </h3>
    <div class="container">
        <div class="col-md-2"></div>
        <div class="col-md-6">
            <div class="app">
                <a href="#" id="start-camera" class="visible">Click para abrir la camara.</a>
                <video id="camera-stream"></video>
                <img id="snap">
                <p id="error-message">
                </p>
                <div class="controls">
                    <a href="#" id="delete-photo" title="Delete Photo" class="disabled">
                        <i class="material-icons">
                            delete
                        </i>
                    </a> <a href="#" id="take-photo" title="Take Photo">
                        <i class="material-icons">
                            camera_alt
                        </i>
                    </a> <a href="#" id="download-photo" download="selfie.png" title="Save Photo"
                            class="disabled"><i class="material-icons">file_download</i></a>
                </div>
                <canvas width="300" height="400"></canvas>
            </div>
        </div>
        <div class="col-md-2"></div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4>Textos encontrados en la imagen:</h4>
                <div id="ResponseTable">
                </div>
            </div>
        </div>
    </div>  
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4>Lista de posibles objetos en la imagen:</h4>
                <div id="ResponseTableOne">
                </div>
            </div>
        </div>
    </div>  
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4>Objeto resaltante en la imagen:</h4>
                <div id="ResponseTableTwo">
                </div>
            </div>
        </div>
    </div> 
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4>Imagenes relacionadas:</h4>
                <div id="ResponseTableThree">
                </div>
            </div>
        </div>
    </div> 
    <script src="~/Scripts/CanvasGoogleScript.js?=1233232"></script>
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <style>
        .contant {
            border: 1px solid #ddd;
            border-radius: 4px;
            width: 500px;
            padding: 20px;
            margin: 0 auto;
            text-align: center;
        }
    </style>
</body>
</html>
